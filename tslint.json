{
    "extends": "tslint:latest",
    "rules": {
        "interface-name": false,
        "member-access": [
            false
        ],
        "no-consecutive-blank-lines": [
            false
        ],
        "no-console": false,
        "no-unused-variable": [
            true
        ],
        "object-literal-shorthand": false,
        "object-literal-sort-keys": false,
        "quotemark": [
            true,
            "single"
        ],
        "trailing-comma": [
            false
        ]
    },
    "jsRules": {
        "quotemark": [
            true,
            "single"
        ],
        "no-consecutive-blank-lines": [
            false
        ],
        "trailing-comma": [
            false
        ]
    },
    "rulesDirectory": [
        "node_modules/tslint-eslint-rules/dist/rules"
    ]
}
